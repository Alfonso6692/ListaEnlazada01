package listasenlazadas;

/**
 *
 * @author ASUS-VASQUEZ
 */
class Nodo {
    String dato;
    Nodo siguiente;

    public Nodo(String dato, Nodo siguiente) {
        this.dato = dato;
        this.siguiente = siguiente;
    }
    
}
